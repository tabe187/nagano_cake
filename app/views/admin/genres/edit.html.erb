<main>
　<div class="container px-5 px-sm-0">

    <h3>ジャンル編集する</h3>
    <%= form_with model: @genre, url: admin_genre_path do |f| %>
    <label for="inputName">ジャンル名</label>
      <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"ジャンル名" %>
      <%= f.submit "変更を保存" %>
    <% end %>
    
  </div>
</main>     